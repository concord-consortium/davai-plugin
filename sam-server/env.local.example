# Local Development Environment Variables
# Copy this file to .env.local and modify as needed

# Database
POSTGRES_CONNECTION_STRING=postgresql://postgres:postgres@host.docker.internal:5432/postgres

# SQS
LLM_JOB_QUEUE_URL=http://host.docker.internal:9324/queue/LLMJobQueue

# Environment
ENVIRONMENT=local

# LangSmith (disabled for local dev)
LANGSMITH_TRACING=false
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_PROJECT=davai-local
LANGSMITH_API_KEY=dummy-key-for-local-dev

# API Secret (for local testing)
DAVAI_API_SECRET=local-dev-secret

# LLM API Keys
OPENAI_API_KEY=sk-your-actual-openai-key-here
GOOGLE_API_KEY=your-actual-google-key-here
